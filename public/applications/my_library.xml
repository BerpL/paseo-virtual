<?xml version="1.0" ?><xml><category name="audio">
        <block type="setTimeout" xmlns="https://developers.google.com/blockly/xml">
  <value name="VALUE">
    <shadow type="math_number">
      <field name="NUM">3</field>
    </shadow>
  </value>
  <statement name="DO">
    <block type="playSound">
      <field name="LOOP">FALSE</field>
      <value name="VALUE">
        <block type="loadSoundAdv">
          <field name="TYPE">MUSIC</field>
          <value name="URL">
            <shadow type="text">
              <field name="TEXT">../audios/1000-1.mp3</field>
            </shadow>
          </value>
        </block>
      </value>
    </block>
  </statement>
</block>
        </category><category name="preloaader">
        <block type="initPreloader" xmlns="https://developers.google.com/blockly/xml">
  <statement name="DO_PROGRESS">
    <block type="setHTMLElemAttribute">
      <field name="ATTRIBUTE">innerHTML</field>
      <field name="PARENT">TRUE</field>
      <value name="VALUE">
        <block type="text_join">
          <mutation items="2"/>
          <value name="ADD0">
            <block type="percentage"/>
          </value>
          <value name="ADD1">
            <block type="text">
              <field name="TEXT">%</field>
            </block>
          </value>
        </block>
      </value>
      <value name="ID">
        <shadow type="text">
          <field name="TEXT"/>
        </shadow>
        <block type="text">
          <field name="TEXT">loading_percentage</field>
        </block>
      </value>
    </block>
  </statement>
  <statement name="DO_END">
    <block type="setHTMLElemStyle">
      <field name="PROPERTY">display</field>
      <field name="PARENT">TRUE</field>
      <value name="VALUE">
        <block type="text">
          <field name="TEXT">none</field>
        </block>
      </value>
      <value name="ID">
        <shadow type="text">
          <field name="TEXT"/>
        </shadow>
        <block type="text">
          <field name="TEXT">preloader_screen</field>
        </block>
      </value>
    </block>
  </statement>
</block>
        </category></xml>